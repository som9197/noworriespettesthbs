<script type="text/javascript">

    $("#firstq").val(""); $("#secondq").val("");

    $("#login").on("click", function (event) {
        event.preventDefault();

        var userLogin = {
            email: $("#firstq").val().trim(),
            thepassword: $("#secondq").val().trim(),
        };

        $.post("/auth/login", userLogin)
           .then(function(data) {
            console.log("Adding");
           });

        $("#firstq").val(""); $("#secondq").val("");

    });

</script>
